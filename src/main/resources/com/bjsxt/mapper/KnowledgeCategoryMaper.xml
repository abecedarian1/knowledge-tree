<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bjsxt.dao.KnowledgeCategoryDao">
    <select id="listAll" resultType="com.bjsxt.entity.KnowledgeCategoryEntity">
           select * from knowledge_category
    </select>
    <select id="singleList" resultType="com.bjsxt.entity.KnowledgeCategoryEntity">
           select * from knowledge_category where id = #{id}
    </select>


</mapper>